    register	PMCD:0:8
    traceconn	PMCD:0:9
    tracepdu	PMCD:0:10
    tracenobuf	PMCD:0:14
    tracebufs	PMCD:0:11
    dumptrace	PMCD:0:12
    dumpconn	PMCD:0:13
    sighup	PMCD:0:15
}

