/*
 * Metrics for the Linux proc PMDA
 *
 * Note:
 *	names and pmids migrated from the Linux PMDA, with the domain
 *	number changed from LINUX (60) to PROC (3)
 */

#ifndef PROC
#define PROC    3
#endif

root {
    cgroup
    proc
    hotproc
}

cgroup {
    subsys
    mounts
    cpuset
    cpuacct
    cpusched
    memory
    netclass
    blkio
}

cgroup.subsys {
    hierarchy		PROC:37:0
    count		PROC:37:1
    num_cgroups		PROC:37:2
    enabled		PROC:37:3
}

cgroup.mounts {
    subsys		PROC:38:0
    count		PROC:38:1
}

cgroup.cpuset {
    cpus		PROC:39:0
    mems		PROC:39:1
}

cgroup.cpuacct {
    stat
    usage		PROC:41:2
    usage_percpu	PROC:41:3
}

cgroup.cpuacct.stat {
    user		PROC:41:0
    system		PROC:41:1
}

cgroup.cpusched {
    shares		PROC:43:0
}

cgroup.memory {
    stat
    usage		PROC:45:80
    limit		PROC:45:81
    failcnt		PROC:45:82
}

