/*
 * Changed GFS2:*:* to root number 115 to remove ASCII parse errors during
 * install of PMDA whilst testing.
 */
gfs2 {
    glocks
    sbstats		GFS2:*:*
    glstats
    tracepoints
    worst_glock		GFS2:*:*
    latency
    control
}

gfs2.glocks {
    total		GFS2:0:0
    shared		GFS2:0:1
    unlocked		GFS2:0:2
    deferred		GFS2:0:3
    exclusive		GFS2:0:4
}

gfs2.glstats {
    total		GFS2:2:0
    trans		GFS2:2:1
