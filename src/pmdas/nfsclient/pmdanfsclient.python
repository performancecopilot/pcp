        ('vfsupdatepage', c_uint),
        ('vfsreadpage', c_uint),
        ('vfsreadpages', c_uint),
        ('vfswritepage', c_uint),
        ('vfswritepages', c_uint),
        ('vfsgetdents', c_uint),
        ('vfssetattr', c_uint),
        ('vfsflush', c_uint),
        ('vfsfsync', c_uint),
        ('vfslock', c_uint),
        ('vfsrelease', c_uint),
        ('congestionwait', c_uint),
