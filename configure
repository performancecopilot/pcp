pcp_ps_all_flags=''
if ps -ef >conftest.out 2>/dev/null
then
    ans=`$awk <conftest.out '
NR == 1			{ if ($1 != "UID" && $1 != "USER") exit
			  if ($2 != "PID") exit
			}
# Unix variants
$2 == 1 && / init/	{ print "OK"; exit }
$2 == 1 && / \/etc\/init/ { print "OK"; exit }
# Interix (aka SFU)
$2 == 0 && /IdleProcess/{ print "OK"; exit }
# Cygwin
/ \/usr\/bin\/bash/	{  print "OK"; exit }
/ \/usr\/sbin\/inetd/	{  print "OK"; exit }'`
    if test "$ans" = OK
    then
	pcp_ps_have_bsd=false
	pcp_ps_all_flags=-ef
	{ echo "$as_me:$LINENO: result: SysV" >&5
echo "${ECHO_T}SysV" >&6; }
    fi
fi
if test -z "$pcp_ps_all_flags" && ps auxww >conftest.out 2>/dev/null
then
    ans=`$awk <conftest.out '
NR == 1			{ if ($1 != "UID" && $1 != "USER") exit
			  if ($2 != "PID") exit
