{
    char buf[17];
    int64_t bozo = 0x12345678abcdLL;
    sprintf(buf, "%lx", bozo);
    if (strcmp(buf, "12345678abcd") == 0)
	printf("\"ld\"\n");
    else {
	sprintf(buf, "%llx", bozo);
	if (strcmp(buf, "12345678abcd") == 0)
	    printf("\"lld\"\n");
    }
    return(0);
}
End-of-File
	(eval $ac_link) 2>&5
	if test -x ./conftest
	then
	    ans=`./conftest`
	    echo "./conftest -> \"$ans\"" >&5
	fi
    fi
fi
if test -z "$ans"
then
    echo 'FATAL ERROR: Cannot determine printf format for int64_t type'
    rm -rf conftest conftest.*
    exit 1
fi
