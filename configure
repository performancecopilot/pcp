	    if test -f $d/man1/man.1.gz
	    then
		have_gzipped_manpages=true
		pcp_man_dir=$d
		break
	    elif test -f $d/man1/man.1.bz2
	    then
		have_bzip2ed_manpages=true
		pcp_man_dir=$d
		break
	    elif test -f $d/man1/man.1
	    then
		pcp_man_dir=$d
		break
	    elif test -f $d/sman1/man.1.gz
