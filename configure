		pcp_man_dir=$d
		break
	    elif test -f $d/sman1/man.1
	    then
		pcp_man_dir=$d
		break
	    elif test -f $d/cat1/man.1
	    then
		pcp_man_dir=$d
		break
	    fi
	done
