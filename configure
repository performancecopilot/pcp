#include <stdint.h>
#include <stddef.h>
#include <stdio.h>
#include <sys/time.h>
typedef struct {
    int64_t sec;
    int32_t nsec;
} __pmTimestamp;
typedef struct {
    __pmTimestamp stamp;
    int numpmid;
} __pmResult;
typedef struct {
    struct timeval stamp;
    int numpmid;
} pmResult;
typedef struct {
    struct timespec stamp;
    int numpmid;
} pmHighResResult;
void
main(void)
{
    __pmResult __rp;
    pmResult rp;
    pmHighResResult hrp;
    int nb;

    nb = (int)offsetof(pmResult,numpmid);
    if (nb < (int)offsetof(pmHighResResult,numpmid))
	nb = (int)offsetof(pmHighResResult,numpmid);
    if (nb > (int)offsetof(__pmResult,numpmid))
	printf("__pmResult pad %d\n", nb - (int)offsetof(__pmResult,numpmid));

    nb = (int)(sizeof(hrp.stamp) - sizeof(hrp.stamp.tv_sec) - sizeof(hrp.stamp.tv_nsec));
    if (nb > 0)
	printf("timespec pad %d\n", nb);
}
End-of-File
