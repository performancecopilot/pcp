ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS build build_cpu build_vendor build_os host host_cpu host_vendor host_os target target_cpu target_vendor target_os PACKAGE_MAJOR PACKAGE_MINOR PACKAGE_REVISION PACKAGE_BUILD PACKAGE_BUILD_DATE package_builder CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT cc MAKE make CXX CXXFLAGS ac_ct_CXX cxx CPP cpp cpp_simple cpp_simple_args LD ld pcp_owner pcp_group TAR tar XMESSAGE xmessage ZIP gzip BZIP2 bzip2 package_maker HDIUTIL hdiutil RPMBUILD rpmbuild RPM rpm rpmprog rpm_version GENDIST gendist MAKEDEPEND makedepend DPKG dpkg LN_S YACC yacc LEX LEXLIB LEX_OUTPUT_ROOT lex AWK awk SED sed ECHO echo echo_n echo_c pcp_ps_have_bsd pcp_ps_all_flags pcp_killall_prog grep EGREP rdynamic_flag solaris_libs lib_for_basename lib_for_dlopen lib_for_math pcp_etc_dir pcp_share_dir pcp_binadm_dir pcp_var_dir pcp_pmcdconf_path pcp_pmcdoptions_path pcp_pmproxyoptions_path pcp_pmdas_dir pcp_lib_dir pcp_lib32_dir X_CFLAGS X_PRE_LIBS X_LIBS X_EXTRA_LIBS pcp_x11_incflags pcp_x11_libflags pcp_x11_extra pcp_x11_pre pcp_x11_appdefs_dir pcp_man_dir have_gzipped_manpages have_bzip2ed_manpages pcp_bin_dir pcp_inc_dir pcp_rc_dir pcp_sysconfig_dir pcp_log_dir pcp_run_dir pcp_tmp_dir pcp_doc_dir pcp_demos_dir ac_xconfirm_prog ac_cv_pthread_lib pcp_mpi_dirs LIBOBJS LTLIBOBJS'
