seq=`basename $0`
echo "QA output created by $seq"

. ./common.python

_cleanup()
{
    cd $here
    $sudo rm -rf $tmp $tmp.*
}

status=0	# success is the default!
trap "_cleanup; exit \$status" 0 1 2 3 15

# real QA test starts here
$python src/test_bad_import.python $tmp.archive

# success, all done
exit
