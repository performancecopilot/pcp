#!/bin/sh
# PCP QA Test No. 801
# Exercise python PMAPI fetchgroup bindings
#
# Copyright (c) 2015 Red Hat.
#

seq=`basename $0`
echo "QA output created by $seq"

. ./common.check
. ./common.setup
[ -f common.python ] || _notrun "Testsuite built without python support"

. ./common.python
status=1	# failure is the default!
$sudo rm -f $tmp.* $seq.full
trap "rm -f $tmp.*; exit \$status" 0 1 2 3 15

$python -c 'from pcp import pmapi' 2>/dev/null
test $? -eq 0 || _notrun 'Python pcp pmapi module is not installed'

# real QA test starts here
$python $here/src/test_pmfg.py >$seq.full 2>&1
_check_unittest pmfg $seq.full
status=$?
exit
